<core:View controllerName="CSOSprintII.tiles.controller.TrackRequests" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml"
xmlns="sap.suite.ui.microchart" xmlns:m="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<m:GenericTile header="Track"
		subheader="Requests" frameType="TwoByOne" press="onPress">
		<m:tileContent>
			<m:TileContent>
				<m:content>
					<ComparisonMicroChart scale="Requests">
						<data>
							<ComparisonMicroChartData title="Responded"
								value="3" color="Good" />
							<ComparisonMicroChartData title="Not Responded"
								value="2" color="Error" />
							<ComparisonMicroChartData title="All"
								value="5"/>
						</data>
					</ComparisonMicroChart>
				</m:content>
			</m:TileContent>
			<m:TileContent>
				<m:content footer="Requests">
					<m:NumericContent value="5"/>
				</m:content>
			</m:TileContent>
		</m:tileContent>
	</m:GenericTile>
</core:View>