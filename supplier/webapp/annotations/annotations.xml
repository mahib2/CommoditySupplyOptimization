						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="DemandDetails/ComponentId"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;ComponentId}"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="DemandQty"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;DemandQty}"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="QuotedPrice"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;QuotedPrice}"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="LeadTime"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;LeadTime}"/>
