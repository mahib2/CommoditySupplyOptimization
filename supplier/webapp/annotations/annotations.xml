									</Record>
								</Collection>
							</PropertyValue>
						</Record>
					</Collection>
				</Annotation>
				<Annotation Term="UI.LineItem">
					<Collection>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="NegotiationId"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;NegotiationId}"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="SupplierId"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;SupplierId}"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Label" String="{@i18n&gt;@SUPPLIER_NAME}"/>
							<PropertyValue Property="Value" Path="SupplierDetails/Name"/>
