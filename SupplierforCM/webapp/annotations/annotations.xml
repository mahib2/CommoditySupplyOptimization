			<Annotations Target="cso_supplier.Supplier">
				<Annotation Term="UI.LineItem">
					<Collection>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="SupplierId"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;@SUPPLIER_ID}"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="SupplierName"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
							<PropertyValue Property="Label" String="{@i18n&gt;@SUPPLIER_NAME}"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="Region"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
						</Record>
						<Record Type="UI.DataFieldForAnnotation">
							<PropertyValue Property="Target" AnnotationPath="@UI.DataPoint#AreaChartQualifier"/>
							<PropertyValue Property="Label" String="Rating"/>
							<Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
						</Record>
					</Collection>
				</Annotation>
				<Annotation Term="UI.DataPoint" Qualifier="AreaChartQualifier">
					<Record>
						<PropertyValue Property="Title" String="Sales Price"/>
						<PropertyValue Property="Value" Path="Ranking"/>
						<PropertyValue Property="Visualization" EnumMember="UI.VisualizationType/Rating"/>
						<PropertyValue Property="MinimumValue" Decimal="1.0"/>
						<PropertyValue Property="MaximumValue" Decimal="5.0"/>
					</Record>
				</Annotation>
				<Annotation Term="UI.HeaderInfo">
					<Record Type="UI.HeaderInfoType">
						<PropertyValue Property="Title">
							<Record Type="UI.DataField">
								<PropertyValue Property="Value" Path="SupplierId"/>
							</Record>
						</PropertyValue>
						<PropertyValue Property="TypeName" String="Suppliers"/>
						<PropertyValue Property="TypeNamePlural" String="Suppliers"/>
					</Record>
				</Annotation>
				<Annotation Term="UI.SelectionFields">
					<Collection>
						<PropertyPath>Region</PropertyPath>
						<PropertyPath>Ranking</PropertyPath>
					</Collection>
				</Annotation>
			</Annotations>
